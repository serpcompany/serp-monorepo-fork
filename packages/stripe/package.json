    "@testing-library/vue": "^8.1.0",
    "@vitest/coverage-v8": "^2.1.5",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.14.0",
    "nuxt": "^3.17.3",
    "typescript": "^5.7.3",
    "vitest": "^2.1.5",
    "vue": "latest"
