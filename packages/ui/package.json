    "@nuxt/icon": "^1.7.5",
    "@nuxt/image": "^1.8.1",
    "@nuxt/kit": "^3.17.3",
    "@nuxt/scripts": "^0.11.1",
    "@nuxt/test-utils": "^3.14.4",
    "@nuxt/ui-pro": "^3.1.2",
    "@nuxtjs/html-validator": "^1.8.2",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@types/eslint__eslintrc": "^2.1.2",
