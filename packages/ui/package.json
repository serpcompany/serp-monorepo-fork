{
  "name": "@serp/ui",
  "version": "0.0.1",
  "type": "module",
  "main": "./nuxt.config.ts",
  "scripts": {
    "build": "nuxt build .playground",
    "eslint": "eslint . --fix",
    "generate": "nuxt generate .playground",
    "postinstall": "nuxt prepare",
    "lint": "prettier . --write --log-level warn && eslint . --fix",
