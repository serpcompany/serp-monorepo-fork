    "jsdom": "^25.0.1",
    "nuxt": "^3.17.3",
    "nuxt-schema-org": "5.0.4",
    "playwright-core": "^1.48.2",
    "prettier-plugin-packagejson": "^2.5.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "typescript": "^5.7.3",
    "vitest": "^2.1.5",
    "vue": "latest",
    "zod": "^3.24.2"
  }
